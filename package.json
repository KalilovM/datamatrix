{
  "name": "datamatrix_new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "lint:fix": "next lint --fix",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:deploy": "npx prisma migrate deploy && npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prisma:push": "npx prisma db push && npx prisma generate",
    "prisma:reset-dev": "run-s prisma:wipe prisma:seed dev",
    "prisma:seed": "tsx prisma/seed.js",
    "prisma:setup": "prisma generate && prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "prisma:wipe": "npx prisma migrate reset --force && npx prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "dotenv": "^16.4.7",
    "jose": "^5.9.6",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-select": "^5.10.0",
    "server-only": "^0.0.1",
    "tsx": "^4.19.2",
    "uuid": "^11.0.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "prisma": "^6.3.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.7.3"
  }
}
